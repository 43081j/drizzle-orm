		"@libsql/client": "^0.6.0",
		"@miniflare/d1": "^2.14.2",
		"@miniflare/shared": "^2.14.2",
		"@planetscale/database": "^1.18.0",
		"@typescript/analyze-trace": "^0.10.1",
		"@vercel/postgres": "^0.8.0",
		"@xata.io/client": "^0.29.4",
		"better-sqlite3": "^9.6.0",
		"dockerode": "^4.0.2",
		"dotenv": "^16.4.5",
		"drizzle-typebox": "workspace:../drizzle-typebox/dist",
